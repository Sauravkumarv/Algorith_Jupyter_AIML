


import pandas as pd
from sklearn.model_selection import train_test_split
from sklearn.linear_model import LogisticRegression
from sklearn.metrics import confusion_matrix,precision_score,accuracy_score,f1_score,recall_score,classification_report
from sklearn.preprocessing import LabelEncoder
from sklearn.neighbors import KNeighborsClassifier
from sklearn.naive_bayes import GaussianNB






dt=pd.read_csv("Iris.csv")
dt=dt.drop(columns=["Id"])
dt["Species"].unique()
le=LabelEncoder()
dt["Species"]=le.fit_transform(dt["Species"])
X=dt.drop(columns=["Species"])
y=dt["Species"]




X_train,X_test,y_train,y_test=train_test_split(X,y,test_size=0.3,random_state=42)
# Logistics Regression
model=LogisticRegression(max_iter=1000)
model.fit(X_train,y_train)
y_pred=model.predict(X_test)
y_pred
confusion_matrix(y_test,y_pred)
# accuracy_score(y_test,y_pred,average='macro')
precision_score(y_test,y_pred,average='macro')
confusion_matrix(y_test,y_pred)


# KNN
knn_classifier=KNeighborsClassifier(n_neighbors=5)
knn_classifier.fit(X_train,y_train)

y_predK=knn_classifier.predict(X_test)
precision_score(y_test,y_predK,average='macro')
accuracy_score(y_test,y_predK)
f1_score(y_test,y_predK,average='macro')
confusion_matrix(y_test,y_predK)



# Naive Bayes
naive_bayes=GaussianNB()
naive_bayes.fit(X_train,y_train)

y_predNB=knn_classifier.predict(X_test)
print(accuracy_score(y_test,y_predNB))
print(f1_score(y_test,y_predNB,average='macro'))
confusion_matrix(y_test,y_predNB)



