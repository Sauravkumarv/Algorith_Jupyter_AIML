   "source": [
    "import pandas as pd\n",
    "df=pd.read_csv(\"raw_data.csv\")\n",
    "df1=df.copy()\n",
    "df2=df1.drop_duplicates()\n",
    "df2=df2.fillna(0)\n",
    "df2=df2.sort_values(\"income\",ascending=False)\n",
    "df2=df2.reset_index(drop=True)\n",
    "\n",
    "df2.to_csv(\"new_raw_data.csv\")"
   ]
  }
