   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "c1bb263a-2a15-4f5f-850c-9297683fa964",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[[1 4 5]\n",
      " [8 9 5]]\n",
      "(2, 3)\n",
      "After Reshape: [[1 4]\n",
      " [5 8]\n",
      " [9 5]]\n"
     ]
    }
   ],
   "source": [
    "import numpy as np\n",
    "arr=np.array([[1,4,5],[8,9,5]])\n",
    "\n",
    "print(f\"Before Reshape: {arr}\\n{arr.shape}\")\n",
    "\n",
    "print(f\"After Reshape: {arr.reshape(3,2)}\")\n"
   ]
  }
